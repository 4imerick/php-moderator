<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>
			{% block title %}
				PHP Moderator 
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link rel="stylesheet" href="{{ asset('assets/css/reset.css') }}"/>
			<link
			rel="stylesheet" href="{{ asset('assets/css/style.css') }}"/>
		{% endblock %}
	</head>
	<body>
		<div class="wrapper">
			<main class="page">
				{% block body %}{% endblock %}
			</main>
		</div>
		{% block javascripts %}
				<script type="text/javascript" src="{{ asset('assets/js/app.js') }}"></script>
				<script type="text/javascript" src="{{ asset('assets/js/modules/form.js') }}"></script>
		{% endblock %}
	</body>
</html>

